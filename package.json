{
	"name": "nft.storage.proxy.client",
	"version": "1.0.0",
	"private": true,
	"description": "nft.storage.proxy.client",
	"main": "src/server.cjs",
	"scripts": {
		"start": "cross-env NODE_ENV=production node public/server",
		"start:dev": "cross-env nodemon public/server",
		"dev:client": "webpack --config webpack.client.config.cjs --watch",
		"dev:server": "cross-env BUNDLE_NAME=server webpack --config webpack.server.config.cjs --watch",
		"serve": "cross-env NODE_ENV=production node --max-old-space-size=4096 public/server",
		"build:client": "webpack --config webpack.client.config.jss",
		"build:server": "cross-env BUNDLE_NAME=server webpack --config webpack.server.config.cjs",
		"production": "webpack --config webpack.config.js --display verbose",
		"dev:styles": "webpack --config webpack.config.js --watch",
    "dev:ts": "tsc && node dist/index.js"
	},
	"dependencies": {
		"axios": "^0.27.2",
		"busboy": "^1.6.0",
		"canvas": "^2.11.0",
		"crypto-js": "^4.1.1",
		"dotenv": "^16.0.3",
		"express": "^4.18.1",
		"images": "^3.2.4",
		"json-bigint": "^1.0.0",
		"nft.storage": "^6.4.0",
		"puppeteer": "^19.5.2",
		"svg-render": "^1.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.18.6",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-proposal-decorators": "^7.8.3",
		"@babel/plugin-proposal-export-namespace-from": "^7.8.3",
		"@babel/plugin-proposal-function-sent": "^7.8.3",
		"@babel/plugin-proposal-json-strings": "^7.8.3",
		"@babel/plugin-proposal-numeric-separator": "^7.8.3",
		"@babel/plugin-proposal-object-rest-spread": "^7.8.3",
		"@babel/plugin-proposal-throw-expressions": "^7.8.3",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-syntax-import-meta": "^7.8.3",
		"@babel/plugin-transform-spread": "7.8.3",
		"@babel/preset-env": "^7.18.6",
		"@babel/register": "^7.8.3",
		"@fullhuman/postcss-purgecss": "^2.0.5",
		"@types/crypto-js": "^4.1.1",
		"@types/express": "^4.17.16",
		"@types/node": "^18.11.18",
		"@types/svg-render": "^1.2.0",
		"autoprefixer": "^9.7.4",
		"babel-loader": "^8.2.5",
		"browserlist": "^1.0.1",
